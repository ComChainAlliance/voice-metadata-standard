{
   "$schema":"http://comchainalliance.org/schema/Channel",
   "definitions":{
      "MemberOrCommunity":{
         "oneOf":[
            {
               "$ref":"http://comchainalliance.org/schema/Member"
            },
            {
               "$ref":"http://comchainalliance.org/schema/Community"
            }
         ]
      }
   },
   "type":"object",
   "properties":{
      "id":{
         "type":"string",
         "description":"The id of the record"
      },
      "channelType":{
         "type":"string",
         "enum":[
            "Shoutdown",
            "Ringdown",
            "Dialtone",
            "IM",
            "SMS"
         ],
         "description":"The type of call that was made (dialtone, ringdown, shoutdown, etc)"
      },
      "identifier":{
         "type":"string",
         "description":"Identifier of the specific channel the user is using for the conversation"
      },
      "owner":{
         "$ref":"http://comchainalliance.org/schema/MemberOrCommunity"
      },
      "regulated":{
         "type":"boolean"
      },
      "label":{
         "type":"string",
         "example":"XYZ Energy",
         "description":"The name on the consuming app for the connection (ex. button name)"
      },
      "device":{
         "$ref":"http://comchainalliance.org/schema/Device"
      },
      "additionalProperty":{
         "type":"array",
         "items":{
            "$ref":"http://schema.org/additionalProperty"
         }
      },
      "extraData":{
         "$ref":"http://comchainalliance.org/schema/ExtraData"
      },
      "comChain":{
         "$ref":"http://comchainalliance.org/schema/ComChain/MIFiD2"
      }
   },
   "required":[
      "identifier",
      "channelType",
      "owner"
   ]
}